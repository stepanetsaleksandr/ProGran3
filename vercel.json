{
  "version": 2,
  "builds": [
    {
      "src": "server/package.json",
      "use": "@vercel/next"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "server/$1"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  },
  "functions": {
    "server/app/api/**/*.ts": {
      "maxDuration": 30
    }
  }
}
